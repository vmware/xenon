[access]
	inheritFrom = All-Projects
[access "refs/heads/*"]
	submit = group Xenon Committers
	label-Code-Review = -2..+2 group Xenon Committers
	submitAs = group xenon-maintainers
	forgeAuthor = group xenon-maintainers
	forgeCommitter = group xenon-maintainers
	push = group xenon-maintainers
[submit]
	action = rebase if necessary
